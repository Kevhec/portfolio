{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@types": [
        "src/types/index.ts"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ]
    },
  }
}